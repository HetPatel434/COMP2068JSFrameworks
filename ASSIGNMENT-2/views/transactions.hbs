<h1>{{title}}</h1>

{{#if transactions.length}}
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Type</th>
      <th>Amount</th>
    </tr>
  </thead>
  <tbody>
    {{#each transactions}}
    <tr>
      <td>{{this.date}}</td>
      <td>{{this.type}}</td>
      <td>{{this.amount}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{else}}
<p>No transactions found.</p>
{{/if}}
